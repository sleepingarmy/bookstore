<%= form_for(@book, :url => admin_books_path) do |f| %> 

	<%= f.label(:title)%> 
	<%= f.text_field(:title)%> 
<br>
	<%= f.label(:author)%> 
	<%= f.text_field(:author)%> 
<br>
	<%= f.label(:genre)%> 
	<%= f.text_field(:genre)%> 
<br>
	<%= f.label(:date_published)%> 
	<%= f.text_field(:date_published)%> 

	<%= f.submit("save") %> 
<% end %>

<br>
<%= link_to("View all books", admin_books_path)%>

